<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
	<file source-language="en" target-language="{locale}" datatype="plaintext" original="{originalFile}" date="{f:format.date(format: 'Y-m-dTH:i:s')}" product-name="form">
		<header/>
		<body>
			<f:for each="{items}" as="item">
				<trans-unit id="{item.identifier}">
					<source>{item.source}</source>
					<target>{item.target}</target>
				</trans-unit>
			</f:for>
		</body>
	</file>
</xliff>
